 int n1, n2;
            n1 = int.Parse(Console.ReadLine());
            n2 = int.Parse(Console.ReadLine());
            if (n1 > n2)
            {
                Console.WriteLine ("{0} é maior que {1}" ,n1,n2 );
            }
            else
            {
                Console.WriteLine("{0} é igual que {1}" ,n1,n2 );
